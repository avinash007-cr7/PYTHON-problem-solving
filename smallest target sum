arr = list(map(int, input("Enter numbers: ").split()))
target = int(input("Enter target: "))

n = len(arr)
min_len = n + 1   # large value

for i in range(n):
    current_sum = 0
    for j in range(i, n):
        current_sum += arr[j]
        if current_sum >= target:
            length = j - i + 1
            if length < min_len:
                min_len = length
            break

if min_len == n + 1:
    print(0)
else:
    print(min_len)
